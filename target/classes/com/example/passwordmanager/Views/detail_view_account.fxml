                  <Image url="@../Images/eye.png" />
               </image>
            </ImageView>
            <ImageView fx:id="cogImageView" fitHeight="20.0" fitWidth="20.0" layoutX="250.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/gear.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="65.0" layoutY="251.0" stylesheets="@../CSS/login_createUser_view.css">
         <children>
            <Label layoutX="8.0" styleClass="text-field-name" text="Note" />
