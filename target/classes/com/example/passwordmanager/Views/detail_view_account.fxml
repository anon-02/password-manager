            <TextField fx:id="accountPasswordVisible" layoutY="17.0" prefHeight="30.0" prefWidth="220.0" styleClass="text-field-content" />
            <PasswordField fx:id="accountPasswordInvisible" editable="false" layoutY="17.0" prefHeight="30.0" prefWidth="220.0" promptText="Enter password" styleClass="text-field-content-password" />
            <Label layoutX="8.0" styleClass="text-field-name" text="Password" />
