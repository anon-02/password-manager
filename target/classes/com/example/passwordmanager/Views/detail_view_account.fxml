<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="676.0" prefWidth="350.0" styleClass="background" stylesheets="@../CSS/login_createUser_view.css" type="AnchorPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane layoutX="65.0" layoutY="80.0" stylesheets="@../CSS/login_createUser_view.css">
         <children>
            <TextField fx:id="entryAccountName" editable="false" layoutY="17.0" prefHeight="24.0" prefWidth="220.0" promptText="Enter  name" styleClass="text-field-content" />
            <Label layoutX="8.0" styleClass="text-field-name" text="Entry name" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="65.0" layoutY="137.0" prefHeight="47.0" prefWidth="220.0" stylesheets="@../CSS/login_createUser_view.css">
         <children>
            <TextField fx:id="accountUsername" editable="false" layoutY="17.0" prefHeight="24.0" prefWidth="220.0" promptText="Enter username" styleClass="text-field-content" />
            <Label layoutX="8.0" styleClass="text-field-name" text="Username" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="65.0" layoutY="194.0" prefHeight="47.0" prefWidth="285.0" stylesheets="@../CSS/login_createUser_view.css">
